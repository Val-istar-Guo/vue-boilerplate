{
  "name": "{{appName}}",
  "version": "0.0.1",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "ava",
    "upgrade": "mili upgrade",
    "clean": "rm -rf ./dist/*",
    "build": "npm run clean & webpack",
    "prepublishOnly": "npm test; npm run build",
    "precommit": "npm test"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "mili": {
    "type": "component",
    "encoding": "{{encoding}}"
